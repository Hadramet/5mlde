{
	"name": "5MLDE (MLOPS) - Group 1 dev space",
	"dockerComposeFile": "docker-compose.yml",
	"service": "jupyter",
	"workspaceFolder": "/workspace",
	"forwardPorts": [8888, 4200, 5000],
	"portsAttributes": {
		"8888": {
			"label": "Jupyter",
			"requireLocalPort": true,
			"onAutoForward": "ignore"
		},
		"4200": {
			"label": "Prefect",
			"requireLocalPort": true,
			"onAutoForward": "ignore"
		},
		"5000": {
			"label": "MLFlow",
			"requireLocalPort": true,
			"onAutoForward": "ignore"
		}
	},

	"customizations": {
		"vscode": {
			"settings": { 
				"python.defaultInterpreterPath": "/opt/conda/bin/python"
			},			
			"extensions": [
				"ms-python.python"
			]
		}
	},
	"remoteUser": "jovyan"
}
